    "npm-run-all": "^4.1.5",
    "protractor": "^7.0.0",
    "rimraf": "^4.4.0",
    "stylelint": "^15.3.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-language-service": "^0.9.9",
    "typescript": "^4.8.2"
  }
