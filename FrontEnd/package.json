    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@types/d3-color": "^3.1.0",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/leaflet": "^1.9.3",
