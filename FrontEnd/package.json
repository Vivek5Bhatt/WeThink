    "socicon": "^3.0.5",
    "style-loader": "^3.3.2",
    "tinymce": "^6.3.2",
    "tslib": "^2.5.0",
    "typeface-exo": "^1.1.13",
    "video-crop": "^1.1.0",
    "zone.js": "^0.13.0"
